to_char(f.from_date_time, 'HH24:MI') as fromtime,
to_char(f.thru_date_time, 'HH24:MI') as thrutime,
substring(f.duration, 1,5) as duration,

to_char(f.feed_extended_from_date_time, 'HH24:MI') as extendedfromtime,
to_char(f.feed_extended_thru_date_time, 'HH24:MI') as extendedthrutime,
substring(f.feed_extended_duration, 1,5) as extendeddurationtime
from failures f
where to_char(f.from_date_time, 'yyyy-mm-dd')::date >= $P{gfFromDate}::date
and  to_char(f.from_date_time, 'yyyy-mm-dd')::date <= $P{gfTodate}::date
and type_of_failure = 'GRID_FAILURE'
and internal_external = 'false']]>
