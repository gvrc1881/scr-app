	<queryString>
		<![CDATA[select $P{division}
||' '|| 'Division' as division,
$P{reportHeader}
||','|| ' '||'Depot:- '|| facility_name ||','|| ' '
||'From Date:- '|| to_char(fpi.start_time, 'yyyy-mm-dd') ||','|| ''||' Thru Date:- '|| to_char(fpi.stop_time, 'yyyy-mm-dd')
as reportHeader,
f.division  ||' ('|| facility_name ||' '||' Depot'||' '||')' as  div_faname,sub_division,f.facility_name,fpi.facility_id,
round ( (EXTRACT(EPOCH From(stop_time - start_time )))::numeric/(3600*24), 6) as duration_days ,
(EXTRACT(EPOCH From(stop_time - start_time ))/3600) as duration_hours,
(EXTRACT(EPOCH From(stop_time - start_time ))/3600*60) as duration_min,
(EXTRACT(EPOCH From(stop_time - start_time ))/3600*60*60) as duration_sec,
start_time,stop_time,section,inspection_by,inspection_type
from foot_patrolling_inspection fpi, facility f
where fpi.facility_id = f.facility_id
and to_char(fpi.start_time, 'yyyy-mm-dd') >=$P{fromDate}
and to_char(fpi.stop_time, 'yyyy-mm-dd') <=$P{toDate}
and sub_division =$P{subDivision}
and f.facility_name=$P{facilityName}
order by sub_division,facility_name,start_time]]>
	</queryString>
