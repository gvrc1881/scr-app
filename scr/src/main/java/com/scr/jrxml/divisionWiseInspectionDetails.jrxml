	<queryString>
		<![CDATA[select  $P{division} ||' '|| 'Division' as division, $P{reportHeader}  as reportHeader,
f.division  ||' ('|| facility_name ||' '||'- Depot'||' '||')' as  div_faname,sub_division,f.facility_name,fpi.facility_id,division,
(EXTRACT(EPOCH From(stop_time - start_time ))/(3600*24)) as duration_days ,
(EXTRACT(EPOCH From(stop_time - start_time ))/3600) as duration_hours,
(EXTRACT(EPOCH From(stop_time - start_time ))/3600*60) as duration_min,
(EXTRACT(EPOCH From(stop_time - start_time ))/3600*60*60) as duration_sec,
start_time,stop_time,location,km,section,inspection_by,inspection_type
from foot_patrolling_inspection fpi, facility f
where fpi.facility_id = f.facility_id
and to_char(fpi.start_time, 'yyyy-mm-dd') >= $P{fromDate}
and to_char(fpi.stop_time, 'yyyy-mm-dd') <= $P{toDate}
and f.division = $P{for_division}
and case when $P{depot} = 'All' then fpi.facility_id in (select facility_id from facility where depot_type  in ('SSP','SP','TSS'))
else fpi.facility_id = $P{depot}
end
order by sub_division,facility_name]]>
	</queryString>
