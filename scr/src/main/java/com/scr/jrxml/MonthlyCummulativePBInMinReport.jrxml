	<queryString>
		<![CDATA[select  d.tpc_board, demanded_spells , demanded_min ,
availed_spells ,sanctioned_spells , sanctioned_min,availed_min
from
(
select tpc_board, case when sum(req_period::numeric) is null then 0 else sum(req_period::numeric) end as demanded_min,count(*) demanded_spells from power_blocks
where
created_date::date = $P{date}::date
group by tpc_board
) d
left outer join
(
select tpc_board,
case when sum(grant_period::numeric) is null then 0 else sum(grant_period::numeric) end as sanctioned_min ,count(*) sanctioned_spells
from power_blocks
where grant_period is not NULL
and created_date::date = $P{date}::date
group by tpc_board
) S on (d.tpc_board = s.tpc_board)
left outer join
(
select tpc_board, sum(to_time - from_time) as availed_min, count(*) availed_spells
from power_blocks
where from_time is not NULL and to_time is not NULL
and created_date::date = $P{date}::date
group by tpc_board
)  A on (d.tpc_board = a.tpc_board)]]>
	</queryString>
