	<queryString>
		<![CDATA[select  (EXTRACT(EPOCH From(sec.date_of_status - fir.date_of_status ))/(3600*24))  as duration_days ,
(EXTRACT(EPOCH From(sec.date_of_status - fir.date_of_status ))/3600) as duration_hours ,sec.asset_type,
sec.sno,sec.date_of_status sec_time,sec.status,sec.asset_id,sec.facility_id,fir.sno,fir.date_of_status fir_time,fir.status,fir.asset_id,fir.facility_id,fir.facility_name
from
(
select  ROW_NUMBER() OVER(ORDER BY date_of_status ASC) sno, b.date_of_status,b.status,b.asset_id,b.facility_id,b.asset_type
from asset_status_update  b
where asset_type = $P{assetType}
) sec
left outer  join
(
select  ROW_NUMBER() OVER(ORDER BY date_of_status ASC) sno,b.asset_type, b.date_of_status,b.status,b.asset_id,b.facility_id,f.facility_name
from asset_status_update  b,facility f
where asset_type = $P{assetType}
and b.facility_id = f.facility_id
) fir on ( -- sec.sno = fir.sno-1 and
 sec.facility_id=fir.facility_id and
 sec.asset_id =fir.asset_id  and
 sec.facility_id=fir.facility_id)]]>
	</queryString>
