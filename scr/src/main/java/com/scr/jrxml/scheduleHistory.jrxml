vash.seq_id,
vash.asset_id,
vash.asset_type,
vash.schedule_code,
vash.schedule_date,
vash.year1,
vash.month1,
vash.week1,
vash.fy,
vash.status,
vash.details_of_maint,
vash.done_by,
vash.initial_of_incharge,
vash.remarks,
vash.pb_operation_seq_id,
vash.facility_id,
vash.facility_name,
vash.facility_type_id,
vash.depot_type
from v_assets_schedule_history vash
where vash.facility_id=$P{depotName}
and vash.asset_type =$P{assetType}
and vash.asset_id = $P{assetId}
--and vash.schedule_date::date = ''::date
and case when $P{scheduleType} != '' then vash.schedule_code = $P{scheduleType}
else vash.schedule_code is not null
end]]>
