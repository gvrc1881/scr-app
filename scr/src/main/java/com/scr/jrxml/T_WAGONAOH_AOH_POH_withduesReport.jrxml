<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="T_WAGONAOH_POH_withduesReport" pageWidth="840" pageHeight="842" whenNoDataType="NoDataSection" columnWidth="800" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="38f87bea-f57e-4bb5-ab53-428b173e319d">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="40"/>
	<property name="ireport.y" value="0"/>
	<parameter name="zone" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="reportHeader" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
