dep_day_pb.Date1
from
(
select facility_name , facility_Id,  count(*) as day_up_asked_count, 0 as day_dn_asked_count, 0 as day_up_availed_count, 0 as day_dn_availed_count,
sum(req_period::Integer) day_up_asked_min,  0 as day_dn_asked_min,  0 as day_local_asked_min,   0 as day_up_availed_min,  0 as day_dn_availed_min,  0 as day_local_availed_min,
