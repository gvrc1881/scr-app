to_char(PB_date,'yyyy-mm-dd') AS Date1
from v_power_block_switch_details
where current_status ='ACTIVE'
and line2 ='UP'
and type_of_operation = 'OHE_POWER_BLOCK'

and to_char(PB_date,'yyyy-mm-dd') = $P{date}
group by facility_name , facility_Id, date1

union all

-- dn asked
select facility_name , facility_Id,  0 as day_up_asked_count, count(*) as day_dn_asked_count, 0 as day_up_availed_count, 0 as day_dn_availed_count,
