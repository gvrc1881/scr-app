amd.strip_date ,amd.equipped_date,
amd.elementary_section,
amd.asset_id,
amd.asset_type,
amd.date_of_commision,
amd.date_of_manufacture,
amd.date_of_received,
amd.make,amd.model,
amd.warranty_amc,
amd.rly_assigned_serial,
p.facility_id,
f.facility_name
from
product p, asset_master_data amd,facility f
where f.facility_name =$P{Depot} and p.product_id =amd.asset_type
--and f.facility_id=p.facility_id
and p.product_type_id ='OHE_FIXED_ASSET' and elementary_section = $P{ElementarySection}]]>
